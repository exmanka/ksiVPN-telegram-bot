CREATE TABLE promocodes_ref (
	id SERIAL PRIMARY KEY,
	phrase VARCHAR(10) UNIQUE,
	client_creator_id INT UNIQUE NOT NULL REFERENCES clients(id) ON DELETE CASCADE
);

