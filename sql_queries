	accessible_client_id INT NOT NULL REFERENCES clients(id) ON DELETE CASCADE,
	promocode_id SMALLINT NOT NULL REFERENCES promocodes_local(id) ON DELETE CASCADE,
	date_of_entry TIMESTAMP DEFAULT NULL
);

