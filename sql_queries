CREATE TABLE promocodes_global (
	id SMALLSERIAL PRIMARY KEY,
	phrase VARCHAR(32) UNIQUE NOT NULL,
	expiration_date TIMESTAMP NOT NULL,
	bonus_time INTERVAL NOT NULL
);

